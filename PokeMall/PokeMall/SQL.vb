            ' Check bag
            Dim CheckBag As String = "SELECT * FROM pn_bag WHERE member='" & sID & "' AND item='" & frmMall.buyID.Text & "'"
            CheckBagCommand.CommandText = CheckBag
            Dim SoldCount As String = "SELECT * FROM pn_mallitems"

            Try
                ' Open connection
                sqlcon.Open()

                ' Execute login
                Try
                    ' Set commands
                    idCommand.Connection = sqlcon
                    DPCommand.Connection = sqlcon
                    ItemCommand.Connection = sqlcon
                    emailCommand.Connection = sqlcon
                    addLogCommand.Connection = sqlcon
                    CheckBagCommand.Connection = sqlcon
                    CountBagCommand.Connection = sqlcon
                    SoldCountCommand.Connection = sqlcon
                    UpdateCountCommand.Connection = sqlcon
                    UpdateQ.Connection = sqlcon
                    idCommand.CommandText = CheckID
                    DPCommand.CommandText = TakeDP
                    emailCommand.CommandText = CheckEmail
                    SoldCountCommand.CommandText = SoldCount

                    ' Set user reader
                    idDReader = idCommand.ExecuteReader

                    ' Execute readers and loop until we get a value
                    Do While idDReader.Read
                        sID = idDReader("id")
                    Loop

                    ' Close once we find value
                    idDReader.Close()

                    ' Set email reader
                    emailReader = emailCommand.ExecuteReader

                    ' Execute reader and loop
                    Do While emailReader.Read
                        sEmail = emailReader("email")
                    Loop

                    ' Close
                    emailReader.Close()

                    ' Set user reader
                    bagReader = CheckBagCommand.ExecuteReader

                    ' Execute readers and loop until we get a value
                    Do While bagReader.Read
                        sItemQ = bagReader("quantity")
                        If sItemQ <> "1" Then
                            frmMall.uq.Text = sItemQ
                        End If
                    Loop

                    ' Close once we find value
                    bagReader.Close()

                    ' Set sold count reader
                    soldReader = SoldCountCommand.ExecuteReader

                    ' Execute readers and loop until we get a value
                    Do While soldReader.Read
                        sCount = soldReader("soldCount")
                        frmMall.soldcount.Text = sCount
                    Loop

                    ' Close once we find value
                    soldReader.Close()

                    ' Maths first
                    Dim q2 As Integer = frmMall.ComboBox1.Text ' val ur buying
                    Dim q1 As Integer = frmMall.uq.Text ' val u have
                    Dim nq As Integer = q1 + q2 'equation

