            Dim emailReader As MySqlDataReader
            Dim bagReader As MySqlDataReader
            Dim soldReader As MySqlDataReader

            ' Static variable
            Dim sID As String
            Dim sEmail As String
            Dim sItemID As String
            Dim sItemQ As String
            Dim sCount As String

            ' Connection
            sqlcon = New MySqlConnection("Server= " & Host & " ; database= " & db & " ; uid= " & user & " ;pwd= " & pass)

            ' SQL
            Dim CheckID As String = "SELECT * FROM pn_members WHERE username = '" & frmLogin.TextBox1.Text & "'"
