    ' Load the connection
    Public Sub LoadCon()
        sqlcon = New MySqlConnection("Server= " & Host & " ; database= " & db & " ; uid= " & user & " ;pwd= " & pass)
        sqlcon.Open()
    End Sub

    ' Close connection
    Public Sub Closeing()
        If ConnectionState.Open Then
